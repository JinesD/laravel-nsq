{
  "name": "jiyis/laravel-nsq",
  "description": "Nsq driver for Laravel Queue",
  "license": "MIT",
  "authors": [
    {
      "name": "Gary F Dong",
      "email": "425995717@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": "^5.1",
    "illuminate/support": "^5.1",
    "illuminate/queue": "^5.1",
    "enqueue/amqp-lib": "0.8.*",
    "queue-interop/amqp-interop": "^0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "illuminate/events": "^5.1",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Jiyis\\Nsq\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jiyis\\Nsq\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Jiyis\\Nsq\\NsqQueueServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
